(in-package :user)
(defvar *pvs-path* "@PVSPATH@")
(eval-when (load eval)
  (require 'tpl-debug)
  #+(or runtime-standard runtime-dynamic)
  (pushnew :runtime *features*)
  #-(or runtime-standard runtime-dynamic)
  (require 'prof)
  (setq excl:*global-gc-behavior* :auto)
  (load (format nil "~a/pvs.system" *pvs-path*)))
(operate-on-system :pvs :compile)

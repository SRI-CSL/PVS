{{#if template_description}}
%
%   Description: This template generates C enumerated types. They are created using a typedef:
%                - typedef struct { c1, c2, ... } enum_type_name;
%
{{/if}}{{#if comment}}/**
 * {{comment}}
 */{{/if}}{{#each recordtype}}
typedef {{#if variables}}struct { {{#each variables}}
    {{#if children}}struct { {{#each children}}
        {{type}} {{name}};{{#if originalType}} //-- {{originalType}}{{/if}}{{/each}} } {{name}}{{
       else}}{{type}} {{name}};{{#if originalType}} //-- {{originalType}}{{/if}}{{/if}}{{/each}}
} {{/if}}{{state_type}};{{/each}}
